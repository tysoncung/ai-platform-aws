# BYOK Multi-Tenant Server
PORT=4000

# AI Gateway (platform default - used when user doesn't provide their own key)
GATEWAY_URL=http://localhost:3000
GATEWAY_API_KEY=your-platform-api-key

# Platform authentication (clients must send this to access the BYOK server)
PLATFORM_API_KEY=your-platform-secret

# Rate limiting
RATE_LIMIT_MAX=100
RATE_LIMIT_WINDOW_MS=60000
