{
  "name": "rag",
  "sourceRoot": "packages/rag/src",
  "targets": {
    "build": {
      "command": "tsc",
      "options": {
        "cwd": "packages/rag"
      }
    },
    "test": {
      "command": "vitest run",
      "options": {
        "cwd": "packages/rag"
      }
    },
    "lint": {
      "command": "eslint src/",
      "options": {
        "cwd": "packages/rag"
      }
    },
    "typecheck": {
      "command": "tsc --noEmit",
      "options": {
        "cwd": "packages/rag"
      }
    }
  }
}
