{
  "name": "agents",
  "sourceRoot": "packages/agents/src",
  "targets": {
    "build": {
      "command": "tsc",
      "options": {
        "cwd": "packages/agents"
      }
    },
    "test": {
      "command": "vitest run",
      "options": {
        "cwd": "packages/agents"
      }
    },
    "lint": {
      "command": "eslint src/",
      "options": {
        "cwd": "packages/agents"
      }
    },
    "typecheck": {
      "command": "tsc --noEmit",
      "options": {
        "cwd": "packages/agents"
      }
    }
  },
  "implicitDependencies": [
    "sdk",
    "rag"
  ]
}
